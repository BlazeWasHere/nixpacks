{
  "providers": [],
  "buildImage": "[build_image]",
  "variables": {
    "NIXPACKS_METADATA": "nim"
  },
  "phases": {
    "build": {
      "name": "build",
      "dependsOn": [
        "install"
      ],
      "cmds": [
        "nimble build -d:release"
      ]
    },
    "install": {
      "name": "install",
      "dependsOn": [
        "setup"
      ],
      "cmds": [
        "nimble install -dy"
      ],
      "onlyIncludeFiles": [
        "nim_prologue.nimble"
      ]
    },
    "setup": {
      "name": "setup",
      "nixPkgs": [
        "nim"
      ],
      "nixOverlays": [],
      "nixpkgsArchive": "[archive]"
    }
  },
  "start": {
    "cmd": "./nim_prologue.exe"
  }
}
